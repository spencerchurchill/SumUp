{
  "name": "Summar",
  "description": "A simple way to preview webpages.",
  "version": "0.6",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "script.js",
        "jquery.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Summar",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}